<?xml version="1.0" encoding="UTF-8"?>
<!-- IAR Project Connection File -->

<iarProjectConnection version="1.2" name="UART Echo" oneShot="true"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="IarProjectConnection_1.2.xsd">

  <device>
    <name>CC1310F128</name>
  </device>

  <extraOptions>
    <args>
      <arg>-I$PROJ_DIR$</arg>
      <arg>-f$SIMPLELINK_CC13XX_CC26XX_SDK_INSTALL_DIR$/kernel/tirtos/builds/CC1310_LAUNCHXL/release/iar/compiler.opt.defs</arg>
      <arg>-DDeviceFamily_CC13X0</arg>
      <arg>-I$SIMPLELINK_CC13XX_CC26XX_SDK_INSTALL_DIR$/source</arg>
      <arg>-I$SIMPLELINK_CC13XX_CC26XX_SDK_INSTALL_DIR$/kernel/tirtos/packages/ti/sysbios/posix</arg>
      <arg>--debug</arg>
      <arg>--silent</arg>
      <arg>-e</arg>
      <arg>--aeabi</arg>
      <arg>--thumb</arg>
      <arg>--diag_suppress=Pa050</arg>
    </args>
  </extraOptions>

  <linkerFile>
    <override>true</override>
    <path>$PROJ_DIR$/$PROJ_FNAME$_CC1310_LAUNCHXL_TIRTOS.icf</path>
  </linkerFile>

  <linkerExtraOptions>
    <args>
      <arg>-f$SIMPLELINK_CC13XX_CC26XX_SDK_INSTALL_DIR$/kernel/tirtos/builds/CC1310_LAUNCHXL/release/iar/linker.cmd</arg>
      <arg>$SIMPLELINK_CC13XX_CC26XX_SDK_INSTALL_DIR$/source/ti/display/lib/display.arm3</arg>
      <arg>$SIMPLELINK_CC13XX_CC26XX_SDK_INSTALL_DIR$/source/ti/mw/lcd/lib/lcd.arm3</arg>
      <arg>$SIMPLELINK_CC13XX_CC26XX_SDK_INSTALL_DIR$/source/ti/grlib/lib/grlib.arm3</arg>
      <arg>$SIMPLELINK_CC13XX_CC26XX_SDK_INSTALL_DIR$/source/ti/drivers/rf/lib/rf_multiMode_cc13x0.arm3</arg>
      <arg>$SIMPLELINK_CC13XX_CC26XX_SDK_INSTALL_DIR$/source/ti/drivers/lib/drivers_cc13x0.arm3</arg>
      <arg>$SIMPLELINK_CC13XX_CC26XX_SDK_INSTALL_DIR$/source/ti/drivers/pdm/lib/pdm_cc13x0.arm3</arg>
      <arg>$SIMPLELINK_CC13XX_CC26XX_SDK_INSTALL_DIR$/kernel/tirtos/packages/ti/dpl/lib/dpl_cc13x0.arm3</arg>
      <arg>$SIMPLELINK_CC13XX_CC26XX_SDK_INSTALL_DIR$/source/ti/devices/cc13x0/driverlib/bin/iar/driverlib.lib</arg>
      <arg>--silent</arg>
      <arg>--cpu=Cortex-M3</arg>
      <arg>--redirect</arg>
      <arg>_Printf=_PrintfSmall</arg>
      <arg>--redirect</arg>
      <arg>_Scanf=_ScanfSmall</arg>
    </args>
  </linkerExtraOptions>

  <heaps>
    <heap> <!-- If omitted, id="0" -->
      <size>0</size>
    </heap>
    <heap id="1">
      <size>0</size>
    </heap>
  </heaps>
  <stacks>
    <stack> <!-- If omitted, id="0" -->
      <size>512</size>
    </stack>
  </stacks>

  <preBuild>
    <command>iarbuild.exe $SIMPLELINK_CC13XX_CC26XX_SDK_INSTALL_DIR$/kernel/tirtos/builds/CC1310_LAUNCHXL/release/iar/tirtos_builds_CC1310_LAUNCHXL_release.ewp Debug -log all</command>
  </preBuild>

  <files>
    <group name="Source files">
      <path copyTo="$PROJ_DIR$/main_tirtos.c">$SIMPLELINK_CC13XX_CC26XX_SDK_INSTALL_DIR$/examples/rtos/CC1310_LAUNCHXL/drivers/uartecho/tirtos/main_tirtos.c</path>
      <path copyTo="$PROJ_DIR$/uartecho.c">$SIMPLELINK_CC13XX_CC26XX_SDK_INSTALL_DIR$/examples/rtos/CC1310_LAUNCHXL/drivers/uartecho/uartecho.c</path>
      <path copyTo="$PROJ_DIR$/CC1310_LAUNCHXL.c">$SIMPLELINK_CC13XX_CC26XX_SDK_INSTALL_DIR$/examples/rtos/CC1310_LAUNCHXL/drivers/uartecho/CC1310_LAUNCHXL.c</path>
      <path copyTo="$PROJ_DIR$/ccfg.c">$SIMPLELINK_CC13XX_CC26XX_SDK_INSTALL_DIR$/examples/rtos/CC1310_LAUNCHXL/drivers/uartecho/ccfg.c</path>
      <path copyTo="$PROJ_DIR$/$PROJ_FNAME$_CC1310_LAUNCHXL_TIRTOS.icf">$SIMPLELINK_CC13XX_CC26XX_SDK_INSTALL_DIR$/examples/rtos/CC1310_LAUNCHXL/drivers/uartecho/tirtos/iar/CC1310_LAUNCHXL_TIRTOS.icf</path>
    </group>

    <group name="Header files">
      <path copyTo="$PROJ_DIR$/Board.h">$SIMPLELINK_CC13XX_CC26XX_SDK_INSTALL_DIR$/examples/rtos/CC1310_LAUNCHXL/drivers/uartecho/Board.h</path>
      <path copyTo="$PROJ_DIR$/CC1310_LAUNCHXL.h">$SIMPLELINK_CC13XX_CC26XX_SDK_INSTALL_DIR$/examples/rtos/CC1310_LAUNCHXL/drivers/uartecho/CC1310_LAUNCHXL.h</path>
    </group>

    <group name="Documentation" build="false">
      <path copyTo="$PROJ_DIR$/README.md">$SIMPLELINK_CC13XX_CC26XX_SDK_INSTALL_DIR$/examples/rtos/CC1310_LAUNCHXL/drivers/uartecho/README.md</path>
      <path copyTo="$PROJ_DIR$/README.html">$SIMPLELINK_CC13XX_CC26XX_SDK_INSTALL_DIR$/examples/rtos/CC1310_LAUNCHXL/drivers/uartecho/README.html</path>
      <path reference="$PROJ_DIR$/Board.html">$SIMPLELINK_CC13XX_CC26XX_SDK_INSTALL_DIR$/source/ti/boards/CC1310_LAUNCHXL/Board.html</path>
    </group>
  </files>
</iarProjectConnection>
